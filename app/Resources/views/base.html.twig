<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %}
        <link rel="stylesheet" href="{{ asset('AppBundle/Resources/css/bootstrap-tour.css') }}"  />
        <link rel="stylesheet" href="{{ asset('AppBundle/Resources/css/bootstrap-theme.css') }}"  />
        <link rel="stylesheet" href="{{ asset('AppBundle/Resources/css/bootstrap.css') }}"  />
        {% endblock %}
        {% block javascripts %}
       	<script type="text/javascript" src="{{ asset('AppBundle/Resources/js/jquery-1.11.0.min.js') }}"></script>
        <script type="text/javascript" src="{{ asset('AppBundle/Resources/js/bootstrap.js') }}"></script>
       	<script type="text/javascript" src="{{ asset('AppBundle/Resources/js/bootstrap-tour.js') }}"></script>
        {% endblock %}
    </head>
    
    <body>
        {% block body %}
        <div class="container">
                {% block nav %}
                {% endblock %}

                {% block content %}
                {% endblock %}

                {% block footer %}
                {% endblock %}
        </div>
        {% endblock %}
    </body>
</html>