<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block javascripts %}
                {% javascripts '@AppBundle/Resources/public/js/jquery-1.11.3.min.js'
                        '@AppBundle/Resources/public/js/bootstrap.min.js' %}
                        <script type="text/javascript" src="{{ asset_url }}"></script>
            {% endjavascripts %}
        {% endblock %}
                {% block stylesheets %}
                {% stylesheets '@AppBundle/Resources/public/css/bootstrap.min.css'
                        filter='cssrewrite' %}
                        <link rel="stylesheet" href="{{ asset_url }}" />
                {% endstylesheets %}
        {% endblock %}
    </head>
    
    <body>
        {% block body %}
        <div class="container">
                {% block nav %}
                {% endblock %}

                {% block content %}
                {% endblock %}

                {% block footer %}
                {% endblock %}
        </div>
        {% endblock %}
    </body>
</html>